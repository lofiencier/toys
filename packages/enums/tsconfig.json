{
  // "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "declaration": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "experimentalDecorators": true,
    "sourceMap": false,
    "baseUrl": "./",
    "skipLibCheck": true,
    "outDir": "lib/",
    "typeRoots": [
      "node_modules/@types",
      "@types"
    ],
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true
  },
  "include": [
    "src/**/*"
  ],
  "noEmit": true,
  "exclude": [
    "node_modules",
  ]
}